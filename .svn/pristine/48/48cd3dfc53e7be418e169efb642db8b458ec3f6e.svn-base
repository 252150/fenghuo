package com.wanjia.infoshopmallmanage.entity;

import java.util.Date;

/**
 * (ScoreLog)实体类
 *
 * @author: wlj
 * @date: 2020-09-27 13:33:45
 */
public class ScoreLog {
    
    private Long id;
    /**
    * 用户id
    */
    private Long userId;
    /**
    * 类型：0:支出；1:收入
    */
    private Integer scoreLogType;
    /**
    * 积分数量
    */
    private Integer scoreNum;
    /**
    * 创建时间
    */
    private Date createTime;
    /**
    * 积分来源(小于100表示收入，大于等于100表示支出)： 0：签到，1:观看广告，2：现金充值，3：赠送，5：消费赠送，101：兑换商品，102：赠送
    */
    private Integer scoreSource;
    /**
    * 备注
    */
    private String remarks;

        
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }
        
    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }
        
    public Integer getScoreLogType() {
        return scoreLogType;
    }

    public void setScoreLogType(Integer scoreLogType) {
        this.scoreLogType = scoreLogType;
    }
        
    public Integer getScoreNum() {
        return scoreNum;
    }

    public void setScoreNum(Integer scoreNum) {
        this.scoreNum = scoreNum;
    }
        
    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }
        
    public Integer getScoreSource() {
        return scoreSource;
    }

    public void setScoreSource(Integer scoreSource) {
        this.scoreSource = scoreSource;
    }
        
    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }

}