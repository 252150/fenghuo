package com.wanjia.infoshopmallmanage.entity;


/**
 * (UserSetting)实体类
 *
 * @author: wlj
 * @date: 2020-09-27 13:33:46
 */
public class UserSetting {
    
    private Long id;
    /**
    * 用户id
    */
    private Long userId;
    /**
    * 收藏是否可见 0:否 1:是
    */
    private Integer collectIsVisible;
    /**
    * 免消息打扰状态： 0：关闭 1：开启
    */
    private Integer noDisturbing;
    /**
    * 是否能够被其他用户通过用户名/手机号搜索到 0:否 1:是
    */
    private Integer isSearch;

        
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }
        
    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }
        
    public Integer getCollectIsVisible() {
        return collectIsVisible;
    }

    public void setCollectIsVisible(Integer collectIsVisible) {
        this.collectIsVisible = collectIsVisible;
    }
        
    public Integer getNoDisturbing() {
        return noDisturbing;
    }

    public void setNoDisturbing(Integer noDisturbing) {
        this.noDisturbing = noDisturbing;
    }
        
    public Integer getIsSearch() {
        return isSearch;
    }

    public void setIsSearch(Integer isSearch) {
        this.isSearch = isSearch;
    }

}